{
  "name": "enquirer",
  "description": "Plugin-based prompt system for node.js",
  "version": "0.1.0",
  "homepage": "https://github.com/jonschlinkert/enquirer",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/enquirer",
  "bugs": {
    "url": "https://github.com/jonschlinkert/enquirer/issues"
  },
  "license": "MIT",
  "files": [
    "LICENSE",
    "README.md",
    "index.js",
    "lib",
    "utils.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "clone-deep": "^0.2.4",
    "co": "^4.6.0",
    "collection-visit": "^0.2.3",
    "component-emitter": "^1.2.1",
    "debug": "^2.2.0",
    "extend-shallow": "^2.0.1",
    "isobject": "^2.1.0",
    "lazy-cache": "^2.0.1",
    "promise-reduce": "^2.1.0"
  },
  "devDependencies": {
    "gulp": "^3.9.1",
    "gulp-eslint": "^3.0.1",
    "gulp-format-md": "^0.1.10",
    "gulp-istanbul": "^1.1.0",
    "gulp-mocha": "^3.0.1",
    "gulp-unused": "^0.2.0",
    "mocha": "^3.0.2"
  },
  "keywords": [
    "answer",
    "answers",
    "ask",
    "checkbox",
    "choice",
    "cli",
    "command",
    "input",
    "inquire",
    "inquirer",
    "interact",
    "list",
    "menu",
    "password",
    "prompt",
    "prompts",
    "question",
    "readline",
    "stdin",
    "stdout",
    "terminal",
    "tty",
    "ui"
  ],
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "lint": {
      "reflinks": true
    },
    "related": {
      "list": []
    },
    "reflinks": [
      "enquirer-prompt",
      "enquirer-question",
      "verb",
      "verb-generate-readme",
      "assemble",
      "generate",
      "inquirer",
      "update"
    ]
  }
}
